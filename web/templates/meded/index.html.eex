<div> List of topics: <%= for topic <- @topics do %> <div style="display:inline;"><%= link topic.topic, to: meded_path(@conn, :bytopic, topic: topic.topic), class: "btn btn-default btn-xs" %></div> <% end %> </div>
<center>
<div style="background-color:#f5f8ff;border-radius: 6px;border-top: 1px solid #e5e5e5;border-right: 1px solid #d0cece;border-left: 1px solid #e5e5e5;border-bottom: 1px solid #d0cece;"">
<%= for video <- @videos do %>
  <h3><%= video.name %></h3><br> 
      <%= raw(video.code)%><br>
      <h4><%= video.discription %></h4>
<% end %> 
</center>
</div>